<p id="notice"><%= notice %></p>
<h1>Support Agents</h1>
<table border="2">
  <thead>
    <tr>
      <th>Name</th>
      <th>Employee number</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @support_agents.each do |support_agent| %>
      <tr>
        <td><%= support_agent.name %></td>
        <td><%= support_agent.employee_number %></td>
        <td><%= link_to 'Show', support_agent %></td>
        <% if current_admin %>
          <td><%= link_to 'Edit', edit_support_agent_path(support_agent) %></td>
          <td><%= link_to 'Destroy', support_agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

